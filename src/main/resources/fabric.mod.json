{
  "schemaVersion": 1,
  "id": "bed_block_beyond",
  "version": "${version}",
  "name": "Bed Block & Beyond",
  "description": "A Minecraft mod expanding the variety of beds available.",
  "authors": [
    "Ignoramuses"
  ],
  "contact": {
    "sources": "https://github.com/Ignoramuses/Bed-Block-Beyond"
  },
  "license": "MIT",
  "icon": "assets/bed_block_beyond/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.ignoramuses.bed_block_beyond.BedBlockBeyond"
    ],

    "preLaunch": [
      "io.github.ignoramuses.bed_block_beyond.BedBlockBeyondPreLaunch"
    ]
  },
  "mixins": [
    "bed_block_beyond.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": ">=${fabric_version}",
    "minecraft": ">=${minecraft_version}",
    "java": ">=${java_version}"
  }
}
